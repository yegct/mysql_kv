MysqlKv::set('hello', 'world', :type => :string)
puts MysqlKv::get('hello')
MysqlKv::set('chris', 123, :type => :integer)
puts MysqlKv::get('chris')
MysqlKv::set('chris', 456, :type => :integer)
puts MysqlKv::get('chris')
MysqlKv::set('nil_key', nil, :type => :nil)
puts MysqlKv::get('nil_key').inspect
MysqlKv::set('true_key', true, :type => :true)
puts MysqlKv::get('true_key').inspect
MysqlKv::set('false_key', nil, :type => :false)
puts MysqlKv::get('false_key').inspect
