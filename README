MysqlKv::set('hello', 'world')
puts MysqlKv::get('hello')
MysqlKv::set('chris', 123)
puts MysqlKv::get('chris')
MysqlKv::set('chris', 456)
puts MysqlKv::get('chris')
MysqlKv::set('nil_key', nil)
puts MysqlKv::get('nil_key').inspect
MysqlKv::set('true_key', true)
puts MysqlKv::get('true_key').inspect
MysqlKv::set('false_key', false)
puts MysqlKv::get('false_key').inspect
